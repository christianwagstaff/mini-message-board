{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "off"
    },
    "globals": {
        "__dirname": true
    }
}
